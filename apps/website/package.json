{
  "name": "refinerdb-website",
  "version": "1.2.7-alpha.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf .cache && rimraf build && rimraf dist && rimraf public/build",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:demos": "node tasks/bundleDemos.js",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss  -i ./styles/global.css -o ./app/styles/global.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^0.19.7",
    "@mapbox/rehype-prism": "^0.8.0",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "daisyui": "^2.15.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
