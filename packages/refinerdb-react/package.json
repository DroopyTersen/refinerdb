{
  "name": "refinerdb-react",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run storybook",
    "build": "tsc --build tsconfig.lib.json",
    "clean": "rimraf lib",
    "tsc:watch": "tsc --build tsconfig.lib.json --watch",
    "storybook": "start-storybook -p 6006",
