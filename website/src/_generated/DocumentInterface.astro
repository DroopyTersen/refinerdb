---
// import type  {GT} from "./generatedTypes.types";
import MethodsTable from "./DocumentMethods.astro";
import PropertiesTable from "./DocumentProperties.astro";
import generatedTypes from "./generatedTypes.json";
// console.log("ðŸš€ | generatedTypes", generatedTypes.typescript);
let name: string = Astro.props.name;
console.log("ðŸš€ | name", name)

let gtClass = generatedTypes?.typescript?.[name];
let methods = [gtClass?.constructorType, ...(gtClass?.methods?.filter(method => method.flags.isPublic) || []) ].filter(Boolean);

---

<h2 id={name.toLowerCase()} class='mt-12 mb-4 text-2xl font-bold'>{name}</h2>

{gtClass?.documentation?.contentsRaw && <p class='mt-4 mb-8'>
{gtClass?.documentation?.contentsRaw}
</p>}

{methods.length > 0 && <MethodsTable methods={methods} /> }
<PropertiesTable properties={[...gtClass?.properties]} />